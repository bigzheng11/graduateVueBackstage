{"ast":null,"code":"import Nothing from \"../../components/../components/index/Nothing.vue\";\nexport default {\n  data() {\n    return {};\n  },\n\n  components: {\n    Nothing\n  }\n};","map":{"version":3,"mappings":"AA4CA;AACA;EACAA;IACA;EACA,CAHA;;EAIAC;IACAC;EADA;AAJA","names":["data","components","Nothing"],"sourceRoot":"src/views/user","sources":["UserList.vue"],"sourcesContent":["<!-- 用户管理组件 -->\n<template>\n   <div class=\"userList\">\n      用户管理页面\n      <el-table\n         :data=\"\n            tableData.filter(\n               (data) =>\n                  !search ||\n                  data.name.toLowerCase().includes(search.toLowerCase())\n            )\n         \"\n         style=\"width: 100%\"\n      >\n         <el-table-column label=\"Date\" prop=\"date\"> </el-table-column>\n         <el-table-column label=\"Name\" prop=\"name\"> </el-table-column>\n         <el-table-column align=\"right\">\n            <template slot=\"header\" slot-scope=\"scope\">\n               <el-input\n                  v-model=\"search\"\n                  size=\"mini\"\n                  placeholder=\"输入关键字搜索\"\n               />\n            </template>\n            <template slot-scope=\"scope\">\n               <el-button\n                  size=\"mini\"\n                  @click=\"handleEdit(scope.$index, scope.row)\"\n                  >Edit</el-button\n               >\n               <el-button\n                  size=\"mini\"\n                  type=\"danger\"\n                  @click=\"handleDelete(scope.$index, scope.row)\"\n                  >Delete</el-button\n               >\n            </template>\n         </el-table-column>\n      </el-table>\n   </div>\n</template>\n\n\n<script>\nimport Nothing from \"../../components/../components/index/Nothing.vue\";\nexport default {\n   data() {\n      return {};\n   },\n   components: {\n      Nothing,\n   },\n};\n</script>\n\n\n<style>\n</style>\n"]},"metadata":{},"sourceType":"module"}