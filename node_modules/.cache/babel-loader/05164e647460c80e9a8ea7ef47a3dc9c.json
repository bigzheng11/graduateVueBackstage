om = document.getElementById(\"main\");\r\n         var myChart = this.$echarts.init(chartDom);\r\n         this.option && myChart.setOption(this.option);\r\n      },\r\n   },\r\n\r\n   destroyed() {\r\n      //离开页面是销毁\r\n      clearInterval(this.timer);\r\n      this.timer = null;\r\n   },\r\n};\r\n</script>\r\n \r\n <style scoped>\r\n/* 卡片 */\r\n.time {\r\n   font-size: 13px;\r\n   color: #999;\r\n}\r\n\r\n.bottom {\r\n   margin-top: 13px;\r\n   line-height: 12px;\r\n}\r\n\r\n.button {\r\n   padding: 0;\r\n   float: right;\r\n}\r\n\r\n.image {\r\n   width: 100%;\r\n   display: block;\r\n}\r\n\r\n.clearfix:before,\r\n.clearfix:after {\r\n   display: table;\r\n   content: \"\";\r\n}\r\n\r\n.clearfix:after {\r\n   clear: both;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}